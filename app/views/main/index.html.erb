<div class="clickable-heading">
  <h1>All People</h1>
</div>
<div class="jumbotron">
  <div class="row">
    <div class="col-xs-6"><h3>Name</h3></div>
    <div class="col-xs-6"><h3>Email</h3></div>
  </div>
  <div class="container" data-bind="foreach: people">
    <div class="row">
      <div class="col-xs-6" data-bind="text:name"></div>
      <div class="col-xs-6" data-bind="text:email"></div>
    </div>
  </div>
  <div class="row new-person">
    <div class="col-xs-6">
      <input class="name form-control" type="text" placeholder="John Smith"/>
    </div>
    <div class="col-xs-6 col-sm-4">
      <input class="email form-control" type="text" placeholder="jsmith@somesite.com"/>
    </div>
    <div class="col-sm-2">
      <button type="button" class="btn btn-default btn-block">Add</button>
    </div>
  </div>
</div>


